Enemy="All enemies"
Hex="All hexes on the grid"
Unit="Both players and enemies"

[rendering]

renderer/rendering_method="mobile"
anti_aliasing/quality/msaa_3d=1
anti_aliasing/quality/screen_space_aa=1
